        "url": "<%= username %>/<%= name %>",
        "type": "git"
  },
  "author": "<%= username %> <<%= email %>>",
  "private": true,
  "main": "./dist/server/main.js",
  "scripts": {
    "dev": "f3 dev",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
